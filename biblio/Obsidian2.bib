  author = {Burgess, H. K. and DeBey, L. B. and Froehlich, H. E. and Schmidt, N. and Theobald, E. J. and Ettinger, A. K. and HilleRisLambers, J. and Tewksbury, J. and Parrish, J. K.},
  date = {2017-04-01},
  journaltitle = {Biological Conservation},
