@article{petersonUnderstandingScopingReviews2017,
  title = {Understanding Scoping Reviews: {{Definition}}, Purpose, and Process},
  shorttitle = {Understanding Scoping Reviews},
  author = {Peterson, Jessica and Pearce, Patricia F. and Ferguson, Laurie Anne and Langford, Cynthia A.},
